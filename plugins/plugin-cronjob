plugin {
    id 3000011
    abstract Linux cronjob check
    netaccess no
    command check-cronjob
    plugin Check.Cronjob
    datatype statistic
    category Linux
    prefer localhost
    description Get statistics and exit code for a cronjob.
}

statistic {
    datatype bigint
    statkey cron_interval
    stattype gauge
    description cron_interval
}

statistic {
    datatype bigint
    statkey unshared_size
    units bytes
    stattype gauge
    description unshared_size
}

