plugin {
    id 3000002
    plugin Varnish4.Check
    command check-varnish4
    datatype statistic
    category System,Webserver,Varnish
    abstract Varnish4 server status
    description Varnish4 statistics
}

statistic {
    statkey cache_hit_ratio
    alias Cache hit ratio
    datatype float
    description Cache hit ratio
}           

statistic {
    statkey cache_hitpass_ratio
    alias Cache hitpass ratio
    datatype float
    description Cache hitpass ratio
}

statistic {
    statkey cache_miss_ratio
    alias Cache miss ratio
    datatype float
    description Cache miss ratio
}

statistic {
    statkey uptime
    alias Child process uptime
    datatype float
    description How long the child process has been running.
    stattype counter
}

statistic {
    statkey sess_conn
    alias Sessions accepted
    datatype float
    description Count of sessions successfully accepted
    stattype counter
}

statistic {
    statkey sess_drop
    alias Sessions dropped
    datatype float
    description Count of sessions silently dropped due to lack of worker thread.
    stattype counter
}

statistic {
    statkey sess_fail
    alias Session accept failures
    datatype float
    description Count of failures to accept TCP connection. Either the client changed its mind, or the kernel ran out of some resource like file descriptors.
    stattype counter
}

statistic {
    statkey client_req_400
    alias Client requests received, subject to 400 errors
    datatype float
    description 400 means we couldn't make sense of the request, it was malformed in some drastic way.
    stattype counter
}

statistic {
    statkey client_req_417
    alias Client requests received, subject to 417 errors
    datatype float
    description 417 means that something went wrong with an Expect: header.
    stattype counter
}

statistic {
    statkey client_req
    alias Good client requests received
    datatype float
    description The count of parseable client requests seen.
    stattype counter
}

statistic {
    statkey cache_hit
    alias Cache hits
    datatype float
    description Count of cache hits. A cache hit indicates that an object has been delivered to a client without fetching it from a backend server.
    stattype counter
}

statistic {
    statkey cache_hitpass
    alias Cache hits for pass
    datatype float
    description Count of hits for pass A cache hit for pass indicates that Varnish is going to pass the request to the backend and this decision has been cached in it self. This counts how many times the cached decision is being used.
    stattype counter
}

statistic {
    statkey cache_miss
    alias Cache misses
    datatype float
    description Count of misses A cache miss indicates the object was fetched from the backend before delivering it to the backend.
    stattype counter
}

statistic {
    statkey backend_conn
    alias Backend conn. success
    datatype float
    description How many backend connections have successfully been established.
    stattype counter
}

statistic {
    statkey backend_unhealthy
    alias Backend conn. not attempted
    datatype float
    description Backend conn. not attempted
    stattype counter
}

statistic {
    statkey backend_busy
    alias Backend conn. too many
    datatype float
    description Backend conn. too many
    stattype counter
}

statistic {
    statkey backend_fail
    alias Backend conn. failures
    datatype float
    description Backend conn. failures
    stattype counter
}

statistic {
    statkey backend_reuse
    alias Backend conn. reuses
    datatype float
    description Count of backend connection reuses This counter is increased whenever we reuse a recycled connection.
    stattype counter
}

statistic {
    statkey backend_recycle
    alias Backend conn. recycles
    datatype float
    description Count of backend connection recycles This counter is increased whenever we have a keep-alive connection that is put back into the pool of connections. It has not yet been used, but it might be, unless the backend closes it.
    stattype counter
}

statistic {
    statkey backend_retry
    alias Backend conn. retry
    datatype float
    description Backend conn. retry
    stattype counter
}

statistic {
    statkey fetch_head
    alias Fetch no body (HEAD)
    datatype float
    description beresp with no body because the request is HEAD.
    stattype counter
}

statistic {
    statkey fetch_length
    alias Fetch with Length
    datatype float
    description beresp.body with Content-Length.
    stattype counter
}

statistic {
    statkey fetch_chunked
    alias Fetch chunked
    datatype float
    description beresp.body with Chunked.
    stattype counter
}

statistic {
    statkey fetch_eof
    alias Fetch EOF
    datatype float
    description beresp.body with EOF.
    stattype counter
}

statistic {
    statkey fetch_bad
    alias Fetch bad T-E
    datatype float
    description beresp.body length/fetch could not be determined.
    stattype counter
}

statistic {
    statkey fetch_none
    alias Fetch no body
    datatype float
    description beresp.body empty
    stattype counter
}

statistic {
    statkey fetch_1xx
    alias Fetch no body (1xx)
    datatype float
    description beresp with no body because of 1XX response.
    stattype counter
}

statistic {
    statkey fetch_204
    alias Fetch no body (204)
    datatype float
    description beresp with no body because of 204 response.
    stattype counter
}

statistic {
    statkey fetch_304
    alias Fetch no body (304)
    datatype float
    description beresp with no body because of 304 response.
    stattype counter
}

statistic {
    statkey fetch_failed
    alias Fetch failed (all causes)
    datatype float
    description beresp fetch failed.
    stattype counter
}

statistic {
    statkey fetch_no_thread
    alias Fetch failed (no thread)
    datatype float
    description beresp fetch failed, no thread available.
    stattype counter
}

statistic {
    statkey pools
    alias Number of thread pools
    datatype float
    description Number of thread pools. See also parameter thread_pools. NB: Presently pools cannot be removed once created.
    stattype gauge
}

statistic {
    statkey threads
    alias Total number of threads
    datatype float
    description Number of threads in all pools. See also parameters thread_pools, thread_pool_min and thread_pool_max.
    stattype gauge
}

statistic {
    statkey threads_limited
    alias Threads hit max
    datatype float
    description Number of times more threads were needed, but limit was reached in a thread pool. See also parameter thread_pool_max.
    stattype counter
}

statistic {
    statkey threads_created
    alias Threads created
    datatype float
    description Total number of threads created in all pools.
    stattype counter
}

statistic {
    statkey threads_destroyed
    alias Threads destroyed
    datatype float
    description Total number of threads destroyed in all pools.
    stattype counter
}

statistic {
    statkey threads_failed
    alias Thread creation failed
    datatype float
    description Number of times creating a thread failed. See VSL::Debug for diagnostics. See also parameter thread_fail_delay.
    stattype counter
}

statistic {
    statkey thread_queue_len
    alias Length of session queue
    datatype float
    description Length of session queue waiting for threads. NB: Only updates once per second. See also parameter queue_max.
    stattype gauge
}

statistic {
    statkey busy_sleep
    alias Number of requests sent to sleep on busy objhdr
    datatype float
    description Number of requests sent to sleep without a worker thread because they found a busy object.
    stattype counter
}

statistic {
    statkey busy_wakeup
    alias Number of requests woken after sleep on busy objhdr
    datatype float
    description Number of requests taken of the busy object sleep list and and rescheduled.
    stattype counter
}

statistic {
    statkey busy_killed
    alias Number of requests killed after sleep on busy objhdr
    datatype float
    description Number of requests killed from the busy object sleep list due to lack of resources.
    stattype counter
}

statistic {
    statkey sess_queued
    alias Sessions queued for thread
    datatype float
    description Number of times session was queued waiting for a thread. See also parameter queue_max.
    stattype counter
}

statistic {
    statkey sess_dropped
    alias Sessions dropped for thread
    datatype float
    description Number of times session was dropped because the queue were too long already. See also parameter queue_max.
    stattype counter
}

statistic {
    statkey n_object
    alias object structs made
    datatype float
    description Number of object structs made
    stattype gauge
}

statistic {
    statkey n_vampireobject
    alias unresurrected objects
    datatype float
    description Number of unresurrected objects
    stattype gauge
}

statistic {
    statkey n_objectcore
    alias objectcore structs made
    datatype float
    description Number of objectcore structs made
    stattype gauge
}

statistic {
    statkey n_objecthead
    alias objecthead structs made
    datatype float
    description Number of objecthead structs made
    stattype gauge
}

statistic {
    statkey n_backend
    alias Number of backends
    datatype float
    description Number of backends known to us.
    stattype gauge
}

statistic {
    statkey n_expired
    alias Number of expired objects
    datatype float
    description Number of objects that expired from cache because of old age.
    stattype gauge
}

statistic {
    statkey n_lru_nuked
    alias Number of LRU nuked objects
    datatype float
    description How many objects have been forcefully evicted from storage to make room for a new object.
    stattype gauge
}

statistic {
    statkey n_lru_moved
    alias Number of LRU moved objects
    datatype float
    description Number of move operations done on the LRU list.
    stattype gauge
}

statistic {
    statkey losthdr
    alias HTTP header overflows
    datatype float
    description HTTP header overflows
    stattype counter
}

statistic {
    statkey s_sess
    alias Total sessions seen
    datatype float
    description Total sessions seen
    stattype counter
}

statistic {
    statkey s_req
    alias Total requests seen
    datatype float
    description Total requests seen
    stattype counter
}

statistic {
    statkey s_pipe
    alias Total pipe sessions seen
    datatype float
    description Total pipe sessions seen
    stattype counter
}

statistic {
    statkey s_pass
    alias Total pass-ed requests seen
    datatype float
    description Total pass-ed requests seen
    stattype counter
}

statistic {
    statkey s_fetch
    alias Total backend fetches initiated
    datatype float
    description Total backend fetches initiated
    stattype counter
}

statistic {
    statkey s_synth
    alias Total synthethic responses made
    datatype float
    description Total synthethic responses made
    stattype counter
}

statistic {
    statkey s_req_hdrbytes
    alias Request header bytes
    datatype float
    description Total request header bytes received
    units bytes
    stattype counter
}

statistic {
    statkey s_req_bodybytes
    alias Request body bytes
    datatype float
    description Total request body bytes received
    units bytes
    stattype counter
}

statistic {
    statkey s_resp_hdrbytes
    alias Response header bytes
    datatype float
    description Total response header bytes transmitted
    units bytes
    stattype counter
}

statistic {
    statkey s_resp_bodybytes
    alias Response body bytes
    datatype float
    description Total response body bytes transmitted
    units bytes
    stattype counter
}

statistic {
    statkey s_pipe_hdrbytes
    alias Pipe request header bytes
    datatype float
    description Total request bytes received for piped sessions
    units bytes
    stattype counter
}

statistic {
    statkey s_pipe_in
    alias Piped bytes from client
    datatype float
    description Total number of bytes forwarded from clients in pipe sessions
    units bytes
    stattype counter
}

statistic {
    statkey s_pipe_out
    alias Piped bytes to client
    datatype float
    description Total number of bytes forwarded to clients in pipe sessions
    units bytes
    stattype counter
}

statistic {
    statkey sess_closed
    alias Session Closed
    datatype float
    description Session Closed
    stattype counter
}

statistic {
    statkey sess_closed_err
    alias Session Closed with error
    datatype float
    description Total number of sessions closed with errors. See sc_* diag counters for detailed breakdown
    stattype counter
}

statistic {
    statkey sess_readahead
    alias Session Read Ahead
    datatype float
    description Session Read Ahead
    stattype counter
}

statistic {
    statkey sess_herd
    alias Session herd
    datatype float
    description Session herd
    stattype counter
}

statistic {
    statkey shm_records
    alias SHM records
    datatype float
    description SHM records
    stattype counter
}

statistic {
    statkey shm_writes
    alias SHM writes
    datatype float
    description SHM writes
    stattype counter
}

statistic {
    statkey shm_flushes
    alias SHM flushes due to overflow
    datatype float
    description SHM flushes due to overflow
    stattype counter
}

statistic {
    statkey shm_cont
    alias SHM MTX contention
    datatype float
    description SHM MTX contention
