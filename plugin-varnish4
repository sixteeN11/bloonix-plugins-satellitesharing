}

statistic {
    statkey backend_req
    alias Backend requests made
    datatype float
    description Backend requests made
    stattype counter
}

statistic {
    statkey n_vcl
    alias Number of loaded VCLs in total
    datatype float
    description Number of loaded VCLs in total
    stattype counter
}

statistic {
    statkey n_vcl_avail
    alias Number of VCLs available
    datatype float
    description Number of VCLs available
    stattype counter
}

statistic {
    statkey n_vcl_discard
    alias Number of discarded VCLs
    datatype float
    description Number of discarded VCLs
    stattype counter
}

statistic {
    statkey bans
    alias Count of bans
    datatype float
    description Number of all bans in system, including bans superseded by newer bans and bans already checked by the ban-lurker.
    stattype gauge
}

statistic {
    statkey bans_completed
    alias Number of bans marked 'completed'
    datatype float
    description Number of bans which are no longer active, either because they got checked by the ban-lurker or superseded by newer identical bans.
    stattype gauge
}

statistic {
    statkey bans_obj
    alias Number of bans using obj.*
    datatype float
    description Number of bans which use obj.* variables. These bans can possibly be washed by the ban-lurker.
    stattype gauge
}

statistic {
    statkey bans_req
    alias Number of bans using req.*
    datatype float
    description Number of bans which use req.* variables. These bans can not be washed by the ban-lurker.
    stattype gauge
}

statistic {
    statkey bans_added
    alias Bans added
    datatype float
    description Counter of bans added to ban list.
    stattype counter
}

statistic {
    statkey bans_deleted
    alias Bans deleted
    datatype float
    description Counter of bans deleted from ban list.
    stattype counter
}

statistic {
    statkey bans_tested
    alias Bans tested against objects (lookup)
    datatype float
    description Count of how many bans and objects have been tested against each other during hash lookup.
    stattype counter
}

statistic {
    statkey bans_obj_killed
    alias Objects killed by bans (lookup)
    datatype float
    description Number of objects killed by bans during object lookup.
    stattype counter
}

statistic {
    statkey bans_lurker_tested
    alias Bans tested against objects (lurker)
    datatype float
    description Count of how many bans and objects have been tested against each other by the ban-lurker.
    stattype counter
}

statistic {
    statkey bans_tests_tested
    alias Ban tests tested against objects (lookup)
    datatype float
    description Count of how many tests and objects have been tested against each other during lookup. 'ban req.url == foo && req.http.host == bar' counts as one in 'bans_tested' and as two in 'bans_tests_tested'
    stattype counter
}

statistic {
    statkey bans_lurker_tests_tested
    alias Ban tests tested against objects (lurker)
    datatype float
    description Count of how many tests and objects have been tested against each other by the ban-lurker. 'ban req.url == foo && req.http.host == bar' counts as one in 'bans_tested' and as two in 'bans_tests_tested'
    stattype counter
}

statistic {
    statkey bans_lurker_obj_killed
    alias Objects killed by bans (lurker)
    datatype float
    description Number of objects killed by ban-lurker.
    stattype counter
}

statistic {
    statkey bans_dups
    alias Bans superseded by other bans
    datatype float
    description Count of bans replaced by later identical bans.
    stattype counter
}

statistic {
    statkey bans_lurker_contention
    alias Lurker gave way for lookup
    datatype float
    description Number of times the ban-lurker had to wait for lookups.
    stattype counter
}

statistic {
    statkey bans_persisted_bytes
    alias Bytes used by the persisted ban lists
    datatype float
    description Number of bytes used by the persisted ban lists.
    units bytes
    stattype gauge
}

statistic {
    statkey bans_persisted_fragmenta
    alias Extra bytes in persisted ban lists due to fragmentation
    datatype float
    description Number of extra bytes accumulated through dropped and completed bans in the persistent ban lists.
    units bytes
    stattype gauge
}

