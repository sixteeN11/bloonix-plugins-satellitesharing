plugin {
    id 3000001
    plugin ASN.Check
    command check-asn
    datatype statistic
    category Network,ASN
    netaccess yes
    worldwide yes
    prefer localhost
    abstract ASN origin check
    description Check Autonomous System Number (ASN) of public facing IP.
}

statistic {
    statkey asn
    stattype string 
    datatype varchar(10)
    alias ASN
    description Autonomous System Number (ASN) of host's public facing IP.
}

statistic {
    statkey time
    alias Response time
    datatype float
    units ms
    description Time in milliseconds waiting for response.
}

